  "scripts": {
    "start": "webpack-dev-server --mode development --server-type https",
    "build": "webpack-cli",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{ts,tsx}'"
  },
  "sideEffects": [
    "./src/index.tsx",
    "./src/i18n.ts",
    "./src/components-care/patches/ImageTypeDeserializer.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "./mktransifex.sh && lint-staged --allow-empty",
      "pre-push": "tsc --noEmit -i --tsBuildInfoFile .tscbuildcache"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --fix",
    "*.json": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=8.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
