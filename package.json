{
  "name": "identity-management-frontend",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@mui/styles": "^5.13.2",
    "@mui/x-date-pickers": "^6.1.0",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "@types/js-cookie": "^2.2.7",
    "@types/marked": "^4.3.1",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-recaptcha": "^2.3.4",
    "@types/xlsx": "^0.0.36",
    "components-care": "github:Samedis-care/react-components#master_dist",
    "http-proxy-middleware": "^2.0.1",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.3.1",
    "i18next-localstorage-backend": "^3.1.3",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.8.3",
    "marked": "4.3.0",
    "mdi-material-ui": "^7.7.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-query": "^3.24.3",
    "react-recaptcha": "^2.3.10",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1",
    "css-loader": "^6.10.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "html-webpack-plugin": "^5.6.0"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --server-type https",
    "build": "webpack-cli",
    "build-new-node": "echo NodeJS 18/19 only! && NODE_OPTIONS=--openssl-legacy-provider npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "update-cc": "npm rm components-care && npm i github:Samedis-care/react-components#master_dist --save --legacy-peer-deps",
    "prepare": "husky"
  },
  "sideEffects": [
    "./src/index.tsx",
    "./src/i18n.ts",
    "./src/components-care/patches/ImageTypeDeserializer.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "./mktransifex.sh && lint-staged --allow-empty",
      "pre-push": "tsc --noEmit -i --tsBuildInfoFile .tscbuildcache"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --fix",
    "*.json": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=8.x"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "cldr-data": "^36.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  }
}
