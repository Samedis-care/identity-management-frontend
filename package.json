{
  "name": "identity-management-frontend",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@mui/styles": "^5.13.2",
    "@mui/x-date-pickers": "^6.1.0",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "@types/js-cookie": "^2.2.7",
    "@types/marked": "^3.0.1",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-recaptcha": "^2.3.4",
    "@types/react-router-dom": "^5.3.0",
    "@types/xlsx": "^0.0.36",
    "components-care": "github:Samedis-care/react-components#master_dist",
    "http-proxy-middleware": "^2.0.1",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.3.1",
    "i18next-localstorage-backend": "^3.1.3",
    "js-cookie": "^3.0.1",
    "marked": "4.0.10",
    "mdi-material-ui": "^7.7.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-query": "^3.24.3",
    "react-recaptcha": "^2.3.10",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "update-cc": "npm rm components-care && npm i github:Samedis-care/react-components#master_dist --save --legacy-peer-deps"
  },
  "sideEffects": [
    "./src/index.tsx",
    "./src/i18n.ts",
    "./src/components-care/patches/ImageTypeDeserializer.ts"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "./mktransifex.sh && lint-staged --allow-empty",
      "pre-push": "tsc -i --tsBuildInfoFile .tscbuildcache"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --max-warnings=0 --fix",
    "*.json": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=7.x"
  },
  "devDependencies": {
    "cldr-data": "^36.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0"
  }
}
